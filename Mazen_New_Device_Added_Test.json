    "Query":  "AuditLogs\r\n| where OperationName contains \"Add device\"\r\n| extend DeviceID = tostring(parse_json(tostring(parse_json(tostring(TargetResources[0].modifiedProperties))[6].newValue))[0])\r\n| extend DeviceName = tostring(parse_json(tostring(parse_json(tostring(TargetResources[0].modifiedProperties))[10].newValue))[0])\r\n| extend DisplayName = tostring(TargetResources[0].displayName)\r\n| extend TimestampAddDevice = TimeGenerated\r\n// | project DeviceID, OperationVersion, DeviceName, DisplayName, TimestampAddDevice\r\n| join kind=inner\r\n    (\r\n    AuditLogs\r\n    | where OperationName contains \"Register device\"\r\n    | extend UPN = tostring(parse_json(tostring(InitiatedBy.user)).userPrincipalName)\r\n    | extend DeviceID = tostring(AdditionalDetails[4].value)\r\n    | extend TimestampRegisterDevice = TimeGenerated\r\n    | extend IPAddress = tostring(parse_json(tostring(InitiatedBy.user)).ipAddress)\r\n    | extend OS = tostring(AdditionalDetails[3].value)\r\n    | extend AdditionalInfo = tostring(AdditionalDetails[0].value)\r\n    // | project DeviceID, OperationVersion, UPN, TimestampRegisterDevice, IPAddress, OS, AdditionalInfo\r\n    ) on DeviceID\r\n// | project TimestampAddDevice, TimestampRegisterDevice, DeviceName, DeviceID, UPN, DisplayName, IPAddress, OS, AdditionalInfo\r\n| extend CustomEntity = strcat(\r\n    \"Device Added on: \", format_datetime(TimestampAddDevice, \u0027dd-MM-yyyy hh:mm:ss\u0027),\r\n    \"\\n Device Addition Result: \", Result,\r\n    \"\\nDevice Registered on: \", format_datetime(TimestampRegisterDevice, \u0027dd-MM-yyyy hh:mm:ss\u0027),\r\n    \"\\n Device Registration Result: \", Result1,\r\n    \"\\nDevice Name/User: \", DeviceName,\r\n    \"\\nDevice Display Name: \", DisplayName,\r\n    \"\\nDevice Operating System: \", OS,\r\n    \"\\nInitiated By: \", UPN\r\n    )",
