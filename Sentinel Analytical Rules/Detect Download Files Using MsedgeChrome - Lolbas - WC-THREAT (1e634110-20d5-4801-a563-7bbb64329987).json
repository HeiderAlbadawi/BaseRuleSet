                          "apiVersion":  "2023-12-01-preview",
                          "properties":  {
                                             "displayName":  "Detect Download Files Using Msedge/Chrome - Lolbas - [WC-THREAT]",
                                             "description":  "This rule Download Files Using Msedge/Chrome - Lolbas (via cmdline)\n",
                                             "severity":  "High",
                                             "enabled":  true,
                                             "query":  "SecurityEvent |  where EventID == 1 | where (CommandLine contains \u0027--headless\u0027 and CommandLine contains \u0027--enable-logging\u0027 and CommandLine contains \u0027--disable-gpu\u0027 and CommandLine contains \u0027--dump-dom\u0027 and (NewProcessName endswith @\u0027\\msedge.exe\u0027 or NewProcessName endswith @\u0027\\chrome.exe\u0027))\n\n",
                                             "queryFrequency":  "PT15M",
                                             "queryPeriod":  "PT15M",
                                             "triggerOperator":  "GreaterThan",
                                             "triggerThreshold":  0,
                                             "suppressionDuration":  "PT5H",
                                             "suppressionEnabled":  false,
                                             "startTimeUtc":  null,
