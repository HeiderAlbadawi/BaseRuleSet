    "Query":  "let ScreenCaptureAlerts = \r\n    SecurityAlert\r\n    | where Type == \"SecurityAlert\"\r\n    | where Tactics has \"Collection\"\r\n    | where AlertName == \"Suspicious screen capture activity\"\r\n    | project \r\n        TimeGenerated,\r\n        DeviceName = CompromisedEntity,\r\n        AlertName,\r\n        AlertLink,\r\n        AccountName = \"\",\r\n        FileName = \"\",\r\n        ProcessCommandLine = \"\",\r\n        SourceTable = \"SecurityAlert\";\r\nlet ScreenCaptureProcesses = \r\n    DeviceProcessEvents\r\n    | where ActionType == \"ProcessCreated\"\r\n    | where ProcessCommandLine has_any (\"Drawing\", \"CopyFromScreen\", \"VirtualScreen\", \"xwd\", \"screencapture\")\r\n    | project \r\n        TimeGenerated,\r\n        DeviceName = AccountDomain,\r\n        AlertName = \"\",\r\n        AlertLink = \"\",\r\n        AccountName,\r\n        FileName,\r\n        ProcessCommandLine,\r\n        SourceTable = \"DeviceProcessEvents\";\r\nScreenCaptureAlerts\r\n| union ScreenCaptureProcesses\r\n| sort by TimeGenerated desc",
