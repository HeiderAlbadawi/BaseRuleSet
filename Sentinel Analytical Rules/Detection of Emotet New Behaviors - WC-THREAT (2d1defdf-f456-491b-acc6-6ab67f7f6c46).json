{
    "contentVersion":  "1.0.0.0",
    "$schema":  "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "resources":  [
                      {
                          "id":  "[concat(resourceId(\u0027Microsoft.OperationalInsights/workspaces/providers\u0027, parameters(\u0027workspace\u0027), \u0027Microsoft.SecurityInsights\u0027),\u0027/alertRules/2d1defdf-f456-491b-acc6-6ab67f7f6c46\u0027)]",
                          "name":  "[concat(parameters(\u0027workspace\u0027),\u0027/Microsoft.SecurityInsights/2d1defdf-f456-491b-acc6-6ab67f7f6c46\u0027)]",
                          "type":  "Microsoft.OperationalInsights/workspaces/providers/alertRules",
                          "kind":  "Scheduled",
                          "apiVersion":  "2023-12-01-preview",
                          "properties":  {
                                             "displayName":  " Detection of Emotet New Behaviors - [WC-THREAT]",
                                             "description":  "Emotet is a malware strain and a cybercriminal operation believed to be based in Ukraine. Emotet, rundll32.exe is in active use. You can detect new behaviors within this rule. ",
                                             "severity":  "High",
                                             "enabled":  true,
                                             "query":  "SecurityEvent | where (CommandLine contains \u0027rundll32.exe\u0027 and ((CommandLine contains \u0027cmd.exe /c\u0027 and CommandLine contains @\u0027ProgramData\\JooSee.dll\u0027 and CommandLine contains \u0027Mimic\u0027) or (CommandLine contains @\u0027ProgramData\\JooSee.dll\u0027 and CommandLine contains \u0027Mimic\u0027) or (CommandLine contains @\u0027ProgramData\\JooSee.dll\u0027 and CommandLine contains \u0027DllRegisterServer\u0027) or (CommandLine contains @\u0027SysWOW64\\Wmstlgnz\\rkxihv.nhw\u0027 and CommandLine contains \u0027CQhUECct\u0027) or (CommandLine contains @\u0027SysWOW64\\Wmstlgnz\\rkxihv.nhw\u0027 and CommandLine contains \u0027DllRegisterServer\u0027)))\n",
                                             "queryFrequency":  "PT15M",
                                             "queryPeriod":  "PT15M",
                                             "triggerOperator":  "GreaterThan",
                                             "triggerThreshold":  0,
                                             "suppressionDuration":  "PT5H",
                                             "suppressionEnabled":  false,
                                             "startTimeUtc":  null,
                                             "tactics":  [
                                                             "Execution"
                                                         ],
                                             "techniques":  [
                                                                "T1059"
                                                            ],
                                             "subTechniques":  [

                                                               ],
                                             "alertRuleTemplateName":  null,
                                             "incidentConfiguration":  {
                                                                           "createIncident":  true,
                                                                           "groupingConfiguration":  {
                                                                                                         "enabled":  true,
                                                                                                         "reopenClosedIncident":  false,
                                                                                                         "lookbackDuration":  "PT5H",
                                                                                                         "matchingMethod":  "AllEntities",
                                                                                                         "groupByEntities":  [

                                                                                                                             ],
                                                                                                         "groupByAlertDetails":  [

                                                                                                                                 ],
                                                                                                         "groupByCustomDetails":  [

                                                                                                                                  ]
                                                                                                     }
                                                                       },
                                             "eventGroupingSettings":  {
                                                                           "aggregationKind":  "SingleAlert"
                                                                       },
                                             "alertDetailsOverride":  null,
                                             "customDetails":  null,
                                             "entityMappings":  null,
                                             "sentinelEntitiesMappings":  null,
                                             "templateVersion":  null
                                         }
                      }
                  ],
    "parameters":  {
                       "workspace":  {
                                         "type":  "String"
                                     }
                   }
}