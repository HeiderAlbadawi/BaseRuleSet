    "Query":  "DeviceProcessEvents\r\n| where TimeGenerated \u003e ago(1d)\r\n| where FileName in~ (\"tasklist.exe\", \"wmic.exe\", \"powershell.exe\", \"cmd.exe\", \"ps.exe\", \"sh.exe\", \"bash.exe\")\r\n   and ProcessCommandLine has_any (\r\n        \"tasklist\",\r\n        \"wmic process\",\r\n        \"Get-Process\",\r\n        \"gwmi win32_process\",\r\n        \"ps -aux\",\r\n        \"ps -ef\",\r\n        \"pgrep\",\r\n        \"pidof\",\r\n        \"top\",\r\n        \"htop\"\r\n     )\r\n| where InitiatingProcessFileName !in~ (\r\n    \"services.exe\", \"explorer.exe\", \"svchost.exe\", \"lsass.exe\", \"csrss.exe\", \r\n    \"dwm.exe\", \"wininit.exe\", \"winlogon.exe\", \"senseir.exe\"\r\n)\r\n| where AccountName !in~ (\r\n    \"NT AUTHORITY\\\\SYSTEM\",\r\n    \"NT AUTHORITY\\\\LOCAL SERVICE\",\r\n    \"NT AUTHORITY\\\\NETWORK SERVICE\",\r\n    \"system\"\r\n)\r\n| where not(ProcessCommandLine has_any (\r\n    \"NinjaRMM\", \"Kaseya\", \"SCCM\", \"SolarWinds\", \"Qualys\", \"McAfee\", \"CrowdStrike\"\r\n))\r\n| project TimeGenerated, DeviceName, AccountName, FileName, ProcessCommandLine, InitiatingProcessFileName, InitiatingProcessCommandLine, FolderPath, ReportId\r\n",
