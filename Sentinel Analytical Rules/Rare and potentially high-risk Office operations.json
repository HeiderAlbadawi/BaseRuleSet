    "Name":  "d543bc09-be12-4932-89e3-35b9d716d989",
    "Query":  "let ManagedAttempts = SigninLogs\n    | union AADNonInteractiveUserSignInLogs\n    | where TimeGenerated \u003e ago(14d)\n    | where tostring(parse_json(DeviceDetail_dynamic).trustType) != \"\" or tostring(parse_json(DeviceDetail_string).trustType) != \"\" or DeviceDetail_dynamic contains \"PII Removed\" or DeviceDetail_string contains \"PII Removed\"\n    | distinct IPAddress;\nOfficeActivity\n| where TimeGenerated \u003e ago(1h)\n| where Operation in~ (\"Add-MailboxPermission\", \"Add-MailboxFolderPermission\", \"Set-Mailbox\", \"New-ManagementRoleAssignment\", \"New-InboxRule\", \"Set-InboxRule\", \"Set-TransportRule\")\n    and not(UserId has_any (\u0027NT AUTHORITY\\\\SYSTEM (Microsoft.Exchange.ServiceHost)\u0027, \u0027NT AUTHORITY\\\\SYSTEM (w3wp)\u0027, \u0027devilfish-applicationaccount\u0027, \u0027NT AUTHORITY\\\\SYSTEM (Microsoft.Exchange.AdminApi.NetCore)\u0027) and Operation in~ (\"Add-MailboxPermission\", \"Set-Mailbox\"))\n| extend IPAddress = iff(\n    ClientIP matches regex @\u0027^\\[[0-9a-fA-F:]+\\]:\\d+$\u0027, // Matches IPv6 with port\n    replace(@\u0027^\\[([0-9a-fA-F:]+)\\]:\\d+$\u0027, @\u0027\\1\u0027, tostring(ClientIP)),\n    iff(\n        ClientIP matches regex @\u0027^\\d{1,3}(\\.\\d{1,3}){3}:\\d+$\u0027, // Matches IPv4 with port\n        replace(@\u0027^(\\d{1,3}(\\.\\d{1,3}){3}):\\d+$\u0027, @\u0027\\1\u0027, ClientIP),\n        \u0027Invalid IP\u0027 // Handle case if it doesn\u0027t match either pattern\n    )\n)\n| extend\n    timestamp = TimeGenerated,\n    AccountCustomEntity = UserId,\n    IPCustomEntity = IPAddress\n| project timestamp,AccountCustomEntity,IPCustomEntity,Operation,UserType,OfficeWorkload,ResultStatus,ResultReasonType,OfficeObjectId, ExternalAccess,OriginatingServer,OrganizationName,AppPoolName,Parameters\n| join kind=leftanti ManagedAttempts on $left.IPCustomEntity == $right.IPAddress",
