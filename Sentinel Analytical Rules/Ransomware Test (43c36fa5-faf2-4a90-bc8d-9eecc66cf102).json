                                             "displayName":  "Ransomware Test",
                                             "description":  "possible ransomware",
                                             "severity":  "High",
                                             "enabled":  true,
                                             "query":  "SecurityEvent\r\n| where TimeGenerated \u003e ago(10m)\r\n| where EventID == 4688  // Process creation\r\n| where ProcessName endswith \"cmd.exe\" or ProcessName endswith \"powershell.exe\" or ProcessName endswith \".bat\"\r\n| where CommandLine has_any (\"ren\", \"rename\") and \r\n       CommandLine has_any (\".locked\", \".encrypted\", \".pay\", \".crypted\", \".enc\")\r\n| project TimeGenerated, Computer, Account, NewProcessName=ProcessName, CommandLine, ParentProcessName\r\n",
                                             "queryFrequency":  "PT5M",
                                             "queryPeriod":  "PT10M",
                                             "triggerOperator":  "GreaterThan",
                                             "triggerThreshold":  0,
                                             "suppressionDuration":  "PT5H",
                                             "suppressionEnabled":  false,
