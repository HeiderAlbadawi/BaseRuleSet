    "Query":  "let threshold = 5;\nDeviceLogonEvents\n| where ActionType == \"LogonFailure\"\n| summarize FailedAttempts = count(), StartTime = min(TimeGenerated), EndTime = max(TimeGenerated) by AccountName, DeviceName, bin(TimeGenerated, 1h)\n| where FailedAttempts \u003e= threshold\n| extend AlertName = \"Test Analytic Rule: Multiple Logon Failures\", \n         Description = strcat(\"Account \", AccountName, \" on \", DeviceName, \" had \", tostring(FailedAttempts), \" failed logon attempts between \", tostring(StartTime), \" and \", tostring(EndTime)),\n         Severity = \"Medium\"\n| project AlertName, Description, Severity, AccountName, DeviceName, FailedAttempts, StartTime, EndTime\n\n",
